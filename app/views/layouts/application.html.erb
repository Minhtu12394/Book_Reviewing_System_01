<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title yield :title %></title>
    <%= render "layouts/rails_default" %>
  </head>
  <body>
    <%= render "layouts/header" %>
    <div class="container">
      <% flash.each do |message_type, content_messages| %>
        <div class="alert alert-<%= message_type %>"><%= content_messages %></div>
      <% end %>
      <%= yield %>
    </div>
  </body>
  <div id="area-modal">
  </div>
</html>
